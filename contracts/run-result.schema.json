{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Standard run result",
  "type": "object",
  "required": [
    "success",
    "metadata",
    "logs"
  ],
  "additionalProperties": false,
  "properties": {
    "success": {
      "type": "boolean"
    },
    "data": {
      "type": "object"
    },
    "error": {
      "type": "object",
      "required": [
        "code",
        "message"
      ],
      "additionalProperties": true,
      "properties": {
        "code": {
          "type": "string"
        },
        "message": {
          "type": "string"
        },
        "details": {}
      }
    },
    "metadata": {
      "type": "object",
      "required": [
        "tool_version",
        "execution_time_ms"
      ],
      "additionalProperties": true,
      "properties": {
        "tool_version": {
          "type": "string"
        },
        "execution_time_ms": {
          "type": "integer",
          "minimum": 0
        }
      }
    },
    "logs": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}